{
  "Tools": {
    "RootPath": ".\\Tools\\",
    "RootAssemblyRelative": true,
    "MKVToolNix": "MKVToolNix",
    "Handbrake": "Handbrake",
    "MediaInfo": "MediaInfo",
    "FFmpeg": "FFMpeg",
    "EchoArgs": "EchoArgs",
    "SevenZip": "7Zip"
  },
  "Encode": {
    "KeepExtensions": "",
    "ReMuxExtensions": ".avi,.m2ts,.ts,.vob,.mp4,.m4v,.asf,.wmv",
    "ReEncodeVideoCodec": "mpeg2video,msmpeg4v3,h264",
    "ReEncodeVideoProfile": "*,*,Constrained Baseline@30",
    "DefaultLanguage": "eng",
    "VideoEncodeQuality": 20,
    "KeepLanguages": "eng,afr,chi,ind",
    "ReEncodeAudioCodec": "flac,mp2,vorbis,wmapro",
    "AudioEncodeCodec": "ac3"
  },
  "App": {
    "MonitorWaitTime": 60,
    "FileRetryWaitTime": 60,
    "FileRetryCount": 30,
    "DeleteEmptyFolders": true,
    "UseSidecarFiles": true,
    "DeleteFailedFiles": true,
    "TestNoModify": false,
    "TestSnippets": false
  }
}